<%- include('partials/header') %>
<h1>Home</h1>
<p>
    <%= startingContent %>
</p>
<% blogItems.forEach(element=> { %>
    <h1><%=element.title%></h1>
    <p><%=element.content.substring(0,100) + " ..."%>
        <a href="/blogList/<%=element.title%>">Read More</a>
    </p>
<% }); %>
<%- include('partials/footer') %>

<a href="/compose" id="composeNew">
    <button class="btn btn-primary">Compose New Blog</button>
</a>